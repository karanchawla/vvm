# For loops: iterate over collections
# parallel for runs iterations concurrently

export { sequential_results, parallel_results }

@translator = { model: opus }

words = ["hello", "goodbye", "thank you"]

# Sequential: one at a time
sequential_results = for word in words {
  @translator `Translate "{word}" to French`
}

# Parallel: all at once
parallel_results = parallel for word in words {
  @translator `Translate "{word}" to Spanish`
}
